<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<AssemblyVersion>0.1</AssemblyVersion>
		<GenerateDocumentationFile>False</GenerateDocumentationFile>
		<FileVersion>0.1</FileVersion>
		<Version>0.1-alpha</Version>
		<Title>ETA invoicing SDK Library</Title>
		<Description>A Library that implements the invoicing SDK of the Egyptian Tax Authority.</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Authors>Ahmed Suror</Authors>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Copyright>Copyrights © Ahmed Suror 2009 - 2023</Copyright>
		<RepositoryUrl>https://github.com/AhmedSuror/ASET</RepositoryUrl>
		<PackageTags>ETA,egyptian tax authority,tax,authority,egypt tax</PackageTags>
		<Product>$(AssemblyName)</Product>
		<RootNamespace>$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
		<PackageProjectUrl>https://github.com/AhmedSuror/ASET</PackageProjectUrl>
		<RepositoryType>git</RepositoryType>
		<PackageReleaseNotes>Early development stage</PackageReleaseNotes>
		<PackageIcon>Logo-128.png</PackageIcon>
		<ApplicationIcon>aset-icon.ico</ApplicationIcon>
	</PropertyGroup>

	<ItemGroup>
	  <Content Include="aset-icon.ico" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Document\" />
	  <Folder Include="Receipt\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\..\LICENSE.txt">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\..\res\Logo-128.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
	</ItemGroup>

</Project>
